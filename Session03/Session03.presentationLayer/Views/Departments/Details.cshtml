@model Department
@{
    ViewData["Title"] = "Details";
}

<h1>@Model.Name Department Details</h1>



<div class="form-group">
    <label asp-for="Code"></label>
    <input readonly class="form-control" asp-for="Code" />
</div>
<div class="form-group my-3">
    <label asp-for="Name"></label>
    <input readonly class="form-control" asp-for="Name" />

</div>
<div class="form-group">
    <label asp-for="Date"></label>
    <input readonly class="form-control" asp-for="Date" />

</div>

<div class="mt-3">
    <a asp-action="Edit" class="btn btn-success " asp-route-id="@Model.Id">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
</div>

